<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.fiveways">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Five Ways To Win At Any Game"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".One"/>
        <activity android:name=".Two"/>
        <activity android:name=".Three"/>
        <activity android:name=".Four"/>
        <activity android:name=".Five"/>
        <activity android:name=".Result"/>
    </application>

</manifest>
